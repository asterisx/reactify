{
    "presets": [
        "@babel/preset-react",
        "@babel/preset-env",
        "@emotion/babel-preset-css-prop"
    ],
    "plugins": [
        [
            "emotion",
            null
        ],
        [
            "@babel/plugin-proposal-class-properties",
            null
        ],
        [
            "react-css-modules",
            null
        ],
        [
            "transform-rename-import",
            {
                "replacements": [
                    {
                        "original": "^(.+?)\\.scss$",
                        "replacement": "$1.css"
                    }
                ]
            }
        ]
    ]
}
